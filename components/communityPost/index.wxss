.block {
  background: #fff;
  margin-bottom: 20rpx;
  padding: 10px 0px;
}
.block:last-of-type {
  margin-bottom: 0rpx;
}
.info {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  padding: 0 40rpx;
}
.info .avatar {
  width: 40px;
  height: 40px;
  border-radius: 50px;
}
.info .detail {
  display: flex;
  flex-direction: column;
  margin-left: 10px;
}
.info .name{
  font-size: 32rpx;
  color: #7594B3;
}
.info .time {
  font-size: 28rpx; 
  color: #999;
  margin-top: 5rpx;
}
.post {
  margin: 0 40rpx;
}
.post .tag { 
  font-size: 32rpx;
  color: #999;
  margin-right: 5rpx;
}
.post .content {
  font-size: 32rpx;
  margin: 20rpx 0px 10rpx 0rpx;
}
.post input {
  border: 1px solid #d9dbde;
  font-size: 14px;
  padding: 2px 5px;
}
.reply {
  font-size: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  margin-top: 10rpx;
}
.reply i {
  font-size: 40rpx;
  color: #666;
}
.post .replyDetail {
  font-size: 28rpx;
  margin-bottom: 5rpx;
}
.post .replyDetail:first-of-type {
  color: #f00;
}
.replyDetail .user, .replyDetail .atUser{
  color: #7594B3;
}
.replyDetail .replyBtn {
  color: #7594B3;
  margin-left: 15rpx;
}


